{
	"name": "lasserafn/php-string-script-language",
	"description": "Detect language/encoding of a string in PHP",
	"keywords": [
		"string",
		"language",
		"php"
	],
	"type": "library",
	"license": "MIT",
	"authors": [
		{
			"name": "Lasse Rafn",
			"email": "lasserafn@gmail.com"
		}
	],
	"require": {
		"php": "^5.6|^7.0|^7.1|^8.0|^8.1|^8.2"
	},
	"require-dev": {
		"satooshi/php-coveralls": "^1.0",
		"phpunit/phpunit": "^5.6",
		"phpunit/phpunit-mock-objects": "3.2.4",
		"sebastian/exporter": "^1.2",
		"doctrine/instantiator": "1.0.5"
	},
	"autoload": {
		"psr-4": {
			"LasseRafn\\": "src/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Initials\\Tests\\": "tests/"
		}
	},
	"config": {
		"sort-packages": true
	}
}
